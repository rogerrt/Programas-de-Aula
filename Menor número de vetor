#include<stdio.h>
#define tam 10

int main()
{
    int vetor[tam], menor, i;

    for ( i = 0; i < tam; i++)
    {
        printf("\nDigite o valor do elemento %d\n", i);
        scanf("%d", &vetor[i]);

        if (vetor[i] < menor)
        {
            menor = vetor[i];
        }
    }
    printf("\nO menor elemento e %d", menor);
    return 0;
}